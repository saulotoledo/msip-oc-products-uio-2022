{
  "name": "products",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "node ace build --production",
    "start": "npm run init-db && node server.js",
    "start-from-build": "node build/server.js",
    "docker-compose-up": "docker-compose --env-file=./.env.docker up",
    "init-db": "node ace migration:fresh --force --seed",
    "dev": "npm run init-db && node ace serve --watch --node-args=\"--inspect=0.0.0.0:9229\"",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "docker:build": "docker build -t ourcatalog/products:latest -f Dockerfile.prod .",
    "docker:tag": "dotenv -- cross-var docker tag ourcatalog/products %DOCKERHUB_USERNAME%/ourcatalog-products",
    "docker:push": "dotenv -- cross-var docker push %DOCKERHUB_USERNAME%/ourcatalog-products",
    "docker:publish": "npm run docker:build && npm run docker:tag && npm run docker:push",
    "test": "PG_DB_NAME=ourcatalog_test node -r @adonisjs/assembler/build/register japaFile.ts",
    "coverage": "nyc npm run test"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.9.5",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "adonis-preset-ts": "^2.1.0",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "http-status": "^1.5.3",
    "japa": "^4.0.0",
    "pino-pretty": "^9.1.1",
    "prettier": "^2.7.1",
    "supertest": "^6.3.1",
    "typescript": "~4.9",
    "youch": "^3.2.2",
    "youch-terminal": "^2.1.5"
  },
  "dependencies": {
    "@adonisjs/core": "^5.8.9",
    "@adonisjs/lucid": "^18.2.0",
    "@adonisjs/repl": "^3.1.11",
    "@adonisjs/shield": "^7.1.0",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^3.1.0",
    "nyc": "^15.1.0",
    "phc-bcrypt": "^1.0.8",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.2",
    "uuid": "^9.0.0"
  }
}
